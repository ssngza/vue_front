ut\r\n              v-if=\"!row.hide && row.dataType === 'input' || row.dataType === undefined \"\r\n              v-model=\"row.value\"\r\n              :placeholder=\"row.label\"\r\n              style=\"width: 200px;\"\r\n              :value=\"row.value\"\r\n              :disabled=\"row.disabled\"\r\n            />\r\n\r\n            <b-form-select\r\n              v-else-if=\"!row.hide && row.dataType === 'select'\"\r\n              v-model=\"row.value\"\r\n              :options=\"row.options\"\r\n              style=\"width: 200px; font-size: 15px; font-family: 'Yu Gothic';\"\r\n              :value=\"row.value\"\r\n            />\r\n            <b-input-group\r\n              v-else-if=\"!row.hide && row.dataType === 'date'\"\r\n              style=\"width: 200px;\"\r\n            >\r\n              <b-form-input\r\n                v-model=\"row.value\"\r\n                type=\"text\"\r\n                disabled\r\n                autocomplete=\"off\"\r\n                show-decade-nav\r\n                :value=\"row.value\"\r\n              />\r\n              <b-input-group-append style=\"height: 37px;\">\r\n                <b-form-datepicker\r\n                  v-model=\"row.value\"\r\n                  show-decade-nav\r\n                  button-only\r\n                  button-variant=\"outline-primary\"\r\n                  right\r\n                  size=\"sm\"\r\n                  locale=\"en-US\"\r\n                  aria-controls=\"example-input\"\r\n                  :disabled=\"row.disabled\"\r\n                />\r\n              </b-input-group-append>\r\n            </b-input-group>\r\n          </b-col>\r\n        </template>\r\n      </div>\r\n\r\n      <b-card-footer class=\"modal-footer\">\r\n        <b-button\r\n          variant=\"primary\"\r\n          size=\"md\"\r\n          class=\"mb-md-1\"\r\n          @click=\"closeModal\"\r\n        >\r\n          취소\r\n        </b-button>\r\n        <b-button\r\n          variant=\"primary\"\